<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="stoica.vladut" id="COMPLAINTS_SEQ">

        <comment>COMPLAINTS_SEQ SEQUENCE CREATION</comment>

        <createSequence incrementBy="1" sequenceName="COMPLAINTS_SEQ"
            startValue="1" />

        <rollback>
            <dropSequence sequenceName="COMPLAINTS_SEQ" />
        </rollback>

        <sql>
            ALTER SEQUENCE ${database.defaultSchemaName}.COMPLAINTS_SEQ CACHE 1500;
        </sql>
    </changeSet>

    <changeSet id="HIBERNATE_SEQUENCE_SYNONYM" author="stoica.vladut">
        <sql>
            create or replace synonym ${database.defaultSchemaName}.hibernate_sequence for COMPLAINTS_SEQ
        </sql>
    </changeSet>

</databaseChangeLog>
